<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".activitys.MainActivity">

    <Button
        android:id="@+id/buttonPastPractice"
        android:layout_width="249dp"
        android:layout_height="157dp"
        android:onClick="onClickPastPractice"
        android:text="@string/pastPractice"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.433"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonNewPractice"
        android:layout_width="249dp"
        android:layout_height="153dp"
        android:onClick="onClickNewPractice"
        android:text="@string/newPractice"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.802"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/dropAllTable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickDrop"
        android:text="DropAllTable"
        app:layout_constraintBottom_toTopOf="@+id/buttonPastPractice"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>